pipeline {
    agent any

    stages {
        stage('git clone') {
            steps {
                echo 'clonning the code'
            }
        }
        stage('build') {
            steps {
                echo 'building the code'
            }
        }
        stage('test') {
            steps {
                echo 'testing the code'
            }
        }
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
